    },
    "instruction": "Choose your answer, then click 'Submit'",
    "_items": [
      {
        "text": "You can use the data for any purpose, even commercially",
        "_shouldBeSelected": true,
        "feedback": ""
      },
      {
        "text": "You can use the data for non-commercial or research purposes only",
        "_shouldBeSelected": false,
        "feedback": ""
      },
      {
        "text": "You can use the data for purposes approved by the publisher",
        "_shouldBeSelected": false,
        "feedback": ""
      }
    ],
